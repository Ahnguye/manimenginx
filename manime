#
# Please see /usr/share/doc/nginx-doc/examples/ for more detailed examples.
##

# Default server configuration
#

upstream project {
    server localhost:8080;

}

server {
    listen 80;
    server_name www.kerarose.com;
    gzip on;

    location / {
        proxy_pass http://project;
    }

    listen 443 ssl; # managed by Certbot
ssl_certificate /etc/letsencrypt/live/kerarose.com/fullchain.pem; # managed by Certbot
ssl_certificate_key /etc/letsencrypt/live/kerarose.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


    if ($scheme != "https") {
        return 301 https://$host$request_uri;
    } # managed by Certbot

}

server {
    listen 80;
    server_name kerarose.com;
    gzip on;

    location / {
        proxy_pass http://project;
    }

    listen 443 ssl; # managed by Certbot
ssl_certificate /etc/letsencrypt/live/kerarose.com/fullchain.pem; # managed by Certbot
ssl_certificate_key /etc/letsencrypt/live/kerarose.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


    if ($scheme != "https") {
        return 301 https://$host$request_uri;
    } # managed by Certbot

}
